#Q17
create table product
(
    product_id int primary key,
    product_name varchar(20),
    unit_price int
);

desc product;

create table sales
(
    seller_id int,
    product_id int,
    buyer_id int,
    sale_date date,
    quantity int,
    price int,
    foreign key(product_id) references product(product_id)
);

desc sales;

